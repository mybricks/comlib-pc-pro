{
  "title": "可编辑表格",
  "namespace": "mybricks.pro-pc.editable-pro-table",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "version": "1.0.65",
  "description": "",
  "icon": "./icon.svg",
  "data": "./data.json",
  "upgrade": "./upgrade.ts",
  "runtime": "./EditableProTable.tsx",
  "editors": "./editors.tsx",
  "inputs": [
    {
      "id": "value",
      "title": "输入数据",
      "rels": ["value"],
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "desc": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "colsCfg",
      "title": "修改列配置",
      "rels": ["colsCfg"],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "colValue",
      "title": "修改列值",
      "rels": ["colValue"],
      "schema": {
        "type": "object",
        "properties": {
          "dataIndex": {
            "type": "string",
            "desc": "目标列的字段"
          },
          "value": {
            "type": "any",
            "desc": "值"
          },
          "rowKey": {
            "type": "string",
            "desc": "当前行的key"
          }
        }
      }
    },
    {
      "id": "submit",
      "title": "提交数据",
      "rels": ["submit"],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "submitWithCheck",
      "title": "提交数据(校验)",
      "rels": ["submitWithCheckSuccess", "submitWithCheckError"],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "addRow",
      "title": "新增一行",
      "rels": ["addRow"],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "cancelRow",
      "title": "取消当前编辑行",
      "rels": ["cancelRow"],
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "editableRows",
      "title": "设置正在编辑的行(key)",
      "rels": ["editableRows"],
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  ],
  "outputs": [
    {
      "id": "submit",
      "title": "输出数据",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "desc": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "submitWithCheckSuccess",
      "title": "提交数据输出",
      "desc": "输出所有字段的当前值。格式为{[key]: {field: 'xxx'}}",
      "schema": {
        "type": "object",
        "properties": {}
      }
    },
    {
      "id": "submitWithCheckError",
      "title": "校验失败输出",
      "desc": "输出校验失败信息",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "name": {
              "type": "array"
            },
            "errors": {
              "type": "array"
            },
            "warnings": {
              "type": "array"
            }
          }
        }
      }
    },
    {
      "id": "value",
      "title": "数据",
      "desc": "关联输入项【输入数据】，获取表格数据，并输出数据内容",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string"
            },
            "desc": {
              "type": "string"
            },
            "createdAt": {
              "type": "string"
            }
          }
        }
      }
    },
    {
      "id": "colsCfg",
      "title": "设置列配置完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "colValue",
      "title": "设置列值完成",
      "schema": {
        "type": "object",
        "properties": {
          "dataIndex": {
            "type": "string",
            "desc": "目标列的字段"
          },
          "value": {
            "type": "any",
            "desc": "值"
          }
        }
      }
    },
    {
      "id": "addRow",
      "title": "新增一行完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "cancelRow",
      "title": "取消当前编辑行完成",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "editableRows",
      "title": "设置正在编辑的行完成",
      "schema": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    }
  ],
  "slots": []
}
