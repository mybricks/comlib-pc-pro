{
  "title": "Markdown预览",
  "namespace": "mybricks.pro-pc.markdown-preview",
  "version": "1.0.3",
  "description": "Markdown",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "runtime": "./runtime.tsx",
  "data":"./data.json",
  "editors": "./editors.ts",
  "inputs": [
    {
      "id": "setContent",
      "title": "设置内容",
      "rels": ["setContentFinish"],
      "schema": {
        "type": "string"
      },
      "desc": "输入Markdown内容"
    },
    {
      "id": "appendContent",
      "title": "增加内容",
      "rels": ["setContentFinish"],
      "schema": {
        "type": "string"
      },
      "desc": "追加Markdown内容"
    }
  ],
  "outputs": [
    {
      "id": "setContentFinish",
      "title": "完成",
      "schema": {
        "type": "string"
      },
      "desc": "输入内容完成"
    }
  ]
}
