{
  "title": "JSON表单",
  "namespace": "mybricks.pro-pc.json-form",
  "version": "1.0.3",
  "description": "JSON表单",
  "author": "MyBricks",
  "author_name": "MyBricks",
  "icon": "./icon.svg",
  "data": "./data.json",
  "runtime": "./runtime.tsx",
  "editors": "./editors.ts",
  "upgrade": "./upgrade.ts",
  "inputs": [
    {
      "id": "setColumns",
      "title": "输入Schema描述",
      "schema": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "title": {
              "type": "string"
            },
            "tooltip": {
              "type": "string"
            },
            "dataIndex": {
              "type": "string"
            },
            "valueType": {
              "type": "enum",
              "items": [
                {
                  "type": "string",
                  "value": "text"
                },
                {
                  "type": "string",
                  "value": "radio"
                },
                {
                  "type": "string",
                  "value": "checkbox"
                },
                {
                  "type": "string",
                  "value": "select"
                },
                {
                  "type": "string",
                  "value": "date"
                },
                {
                  "type": "string",
                  "value": "treeSelect"
                },
                {
                  "type": "string",
                  "value": "group"
                },
                {
                  "type": "string",
                  "value": "其他各类表单项"
                }
              ]
            },
            "fieldProps": {
              "type": "object"
            },
            "formItemProps": {
              "type": "object"
            },
            "dependencies": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "columns": {
              "type": "array",
              "items": {
                "type": "object"
              }
            },
            "colProps": {
              "type": "array",
              "items": {
                "type": "string"
              }
            },
            "rowProps": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          }
        }
      },
      "desc": "输入符合定义的Schema描述"
    },
    {
      "id": "setFieldsValue",
      "title": "设置表单数据",
      "schema": {
        "type": "object",
        "properties": {}
      },
      "desc": "给表单设置数据"
    },
    {
      "id": "submit",
      "title": "提交表单",
      "rels": [
        "onFinishForRels"
      ],
      "schema": {
        "type": "any"
      }
    }
  ],
  "outputs": [
    {
      "id": "onFinish",
      "title": "提交数据输出",
      "schema": {
        "type": "object"
      }
    },
    {
      "id": "onFinishForRels",
      "title": "提交数据输出",
      "schema": {
        "type": "object"
      }
    },
    {
      "id": "onReset",
      "title": "重置表单输出",
      "schema": {
        "type": "any"
      }
    },
    {
      "id": "onValuesChange",
      "title": "数据变化",
      "schema": {
        "type": "object",
        "properties": {
          "changedValues": {
            "type": "object"
          },
          "allValues": {
            "type": "object"
          }
        }
      }
    }
  ]
}